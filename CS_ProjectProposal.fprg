            <output expression="&quot;Ooh! English, I love this one!&quot;" newline="True"/>
            <output expression="&quot;~~~Make sure to answer with only CAPITAL letters!!~~~&quot;" newline="True"/>
            <output expression="&quot;First question, What are at least 3 elements of a short story?&quot;" newline="True"/>
            <output expression="&quot;A. Theme, Setting, Characters&quot;" newline="True"/>
            <output expression="&quot;B. Plot, Conflict, Laptop&quot;" newline="True"/>
