                    <assign variable="englishPoints" expression="englishPoints + 1"/>
                </then>
                <else>
                    <output expression="&quot;Aww.. Sorry that's incorrect! The right answer was B.&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
    <function name="leaderboard" type="None" variable="">
        <parameters>
            <parameter name="bestSubject" type="String" array="False"/>
            <parameter name="secondBestSubject" type="String" array="False"/>
            <parameter name="worstSubject" type="String" array="False"/>
            <parameter name="worstScore" type="Integer" array="False"/>
            <parameter name="secondBestScore" type="Integer" array="False"/>
            <parameter name="bestScore" type="Integer" array="False"/>
            <parameter name="leaderboardOption" type="String" array="False"/>
            <parameter name="tiedBestSubject" type="String" array="False"/>
            <parameter name="tiedBestScore" type="Integer" array="False"/>
            <parameter name="tiedWorstSubject" type="String" array="False"/>
            <parameter name="tiedWorstScore" type="Integer" array="False"/>
        </parameters>
        <body>
            <while expression="leaderboardOption == &quot;Done&quot;">
                <if expression="leaderboardOption == &quot;Yes&quot;">
