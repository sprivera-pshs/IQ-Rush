            <output expression="&quot;B. Words that connect two independent clauses&quot;" newline="True"/>
            <output expression="&quot;C. Words that show emotion&quot;" newline="True"/>
            <output expression="&quot;D. Words that replace nouns&quot;" newline="True"/>
            <if expression="answer3 == &quot;B&quot;">
                <then>
