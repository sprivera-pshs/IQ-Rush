            <output expression="&quot;B. The cat slept on the mat.&quot;" newline="True"/>
            <output expression="&quot;C. The classroom was a zoo.&quot;" newline="True"/>
            <output expression="&quot;D. The rain fell gently&quot;" newline="True"/>
            <if expression="answer2 == &quot;C&quot;">
                <then>
