    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="userName, bestSubject, secondBestSubject, worstSubject, leaderboardOption, tiedBestSubject, tiedWorstSubject" type="String" array="False" size=""/>
            <declare name="gameIteration, englishOption, mathOption, scienceOption, notDone, done" type="Boolean" array="False" size=""/>
            <declare name="subjectOption, englishScore, mathScore, scienceScore, bestScore, secondBestScore, worstScore, tiedBestScore, tiedWorstScore" type="Integer" array="False" size=""/>
            <output expression="&quot;Hello! Welcome to our game, uh... What's your name again?&quot;" newline="True"/>
            <input variable="userName"/>
            <output expression="&quot;Ohh! right, right.. Anyway, welcome to our game, &quot; &amp; userName &amp; &quot;. In this game, we can help you review your subjects, so that you won't have to struggle anymore! (I hope) :)&quot;" newline="True"/>
            <output expression="&quot;Anyway, we are IQX, here to present.. IQ Rush!&quot;" newline="True"/>
            <output expression="&quot;...&quot;" newline="True"/>
            <assign variable="notDone" expression="False"/>
            <assign variable="done" expression="True"/>
            <assign variable="englishOption" expression="False"/>
            <assign variable="mathOption" expression="False"/>
            <assign variable="scienceOption" expression="False"/>
            <assign variable="gameIteration" expression="False"/>
            <while expression="gameIteration == False">
                <output expression="&quot;Let's just start.. What subject would you like to review? (Options : Input 1 for English, 2 for Math, 3 for Science &amp; 4 If you are done. (Only choose 4. if you are done with all of it!!)) :&quot;" newline="True"/>
                <input variable="subjectOption"/>
                <if expression="(subjectOption ==  1) and (englishOption == notDone)">
                    <then>
                        <call expression="englishQuiz"/>
                        <assign variable="englishScore" expression="englishQuiz()"/>
                        <output expression="englishScore" newline="True"/>
                        <assign variable="englishOption" expression="done"/>
                    </then>
                    <else>
                        <if expression="(subjectOption == 2) and (mathOption == notDone)">
                            <then>
                                <call expression="mathQuiz"/>
                                <assign variable="mathScore" expression="mathQuiz()"/>
                                <output expression="mathScore" newline="True"/>
                                <assign variable="mathOption" expression="done"/>
                            </then>
                            <else>
                                <if expression="(subjectOption == 3) and (scienceOption == notDone)">
                                    <then>
                                        <call expression="scienceQuiz()"/>
                                        <assign variable="scienceScore" expression="scienceQuiz()"/>
                                        <output expression="scienceScore" newline="True"/>
                                        <assign variable="scienceOption" expression="done"/>
                                    </then>
                                    <else>
                                        <if expression="(subjectOption == 4) and (mathOption == done) and (englishOption == done) and (scienceOption == done)">
                                            <then>
                                                <assign variable="gameIteration" expression="True"/>
                                            </then>
                                            <else>
                                                <output expression="&quot;Hm.. I think you spelt it wrong, typed in a different subject from the options or the subject you chose was already chosen before.&quot;" newline="True"/>
                                                <output expression="&quot;Please try again, thank you! ^^&quot;" newline="True"/>
                                            </else>
                                        </if>
                                    </else>
                                </if>
                            </else>
                        </if>
                    </else>
                </if>
            </while>
            <if expression="(englishScore &gt; mathScore) and (englishScore &gt; scienceScore)">
                <then>
                    <assign variable="bestScore" expression="englishScore"/>
                    <assign variable="bestSubject" expression="&quot;English&quot;"/>
                    <if expression="mathScore &gt; scienceScore">
                        <then>
                            <assign variable="secondBestScore" expression="mathScore"/>
                            <assign variable="worstScore" expression="scienceScore"/>
                            <assign variable="secondBestSubject" expression="&quot;Math&quot;"/>
                            <assign variable="worstSubject" expression="&quot;Science&quot;"/>
                        </then>
                        <else>
                            <if expression="scienceScore &gt; mathScore">
                                <then>
                                    <assign variable="secondBestScore" expression="scienceScore"/>
                                    <assign variable="worstScore" expression="mathScore"/>
                                    <assign variable="secondBestSubject" expression="&quot;Science&quot;"/>
                                    <assign variable="worstSubject" expression="&quot;Math&quot;"/>
                                </then>
                                <else>
                                    <assign variable="tiedWorstSubject" expression="&quot;Science&quot;"/>
                                    <assign variable="tiedWorstSubject" expression="&quot;Math&quot;"/>
                                    <assign variable="tiedWorstScore" expression="scienceScore"/>
                                    <assign variable="tiedWorstScore" expression="mathScore"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </then>
                <else>
                    <if expression="(mathScore &gt; englishScore) and (mathScore &gt; scienceScore)">
                        <then>
                            <assign variable="bestScore" expression="mathScore"/>
                            <assign variable="bestSubject" expression="&quot;Math&quot;"/>
                            <if expression="englishScore &gt; scienceScore">
                                <then>
                                    <assign variable="secondBestScore" expression="englishScore"/>
                                    <assign variable="worstScore" expression="scienceScore"/>
                                    <assign variable="secondBestSubject" expression="&quot;English&quot;"/>
                                    <assign variable="worstSubject" expression="&quot;Science&quot;"/>
                                </then>
                                <else>
                                    <if expression="scienceScore &gt; englishScore">
                                        <then>
                                            <assign variable="secondBestScore" expression="scienceScore"/>
                                            <assign variable="worstScore" expression="englishScore"/>
                                            <assign variable="secondBestSubject" expression="&quot;Science&quot;"/>
                                            <assign variable="worstSubject" expression="&quot;English&quot;"/>
                                        </then>
                                        <else>
                                            <assign variable="tiedWorstScore" expression="scienceScore"/>
                                            <assign variable="tiedWorstScore" expression="englishScore"/>
                                            <assign variable="tiedWorstSubject" expression="&quot;Science&quot;"/>
                                            <assign variable="tiedWorstSubject" expression="&quot;English&quot;"/>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </then>
                        <else>
                            <if expression="(scienceScore &gt; englishScore) and (scienceScore &gt; mathScore)">
                                <then>
                                    <assign variable="bestScore" expression="scienceScore"/>
                                    <assign variable="bestSubject" expression="&quot;Science&quot;"/>
                                    <if expression="mathScore &gt; englishScore">
                                        <then>
                                            <assign variable="secondBestSubject" expression="&quot;Math&quot;"/>
                                            <assign variable="worstSubject" expression="&quot;English&quot;"/>
                                            <assign variable="secondBestScore" expression="mathScore"/>
                                            <assign variable="worstScore" expression="englishScore"/>
                                        </then>
                                        <else>
                                            <if expression="englishScore &gt; mathScore">
                                                <then>
                                                    <assign variable="secondBestSubject" expression="&quot;Math&quot;"/>
                                                    <assign variable="worstSubject" expression="&quot;English&quot;"/>
                                                    <assign variable="secondBestScore" expression="mathScore"/>
                                                    <assign variable="worstScore" expression="englishScore"/>
                                                </then>
                                                <else>
                                                    <assign variable="tiedWorstSubject" expression="&quot;Math&quot;"/>
                                                    <assign variable="tiedWorstSubject" expression="&quot;English&quot;"/>
                                                    <assign variable="tiedWorstScore" expression="mathScore"/>
                                                    <assign variable="tiedWorstScore" expression="englishScore"/>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </then>
                                <else>
                                    <if expression="englishScore == mathScore">
                                        <then>
                                            <if expression="englishScore == scienceScore">
                                                <then>
                                                    <assign variable="tiedBestSubject" expression="&quot;Math&quot;"/>
                                                    <assign variable="tiedBestSubject" expression="&quot;English&quot;"/>
                                                    <assign variable="tiedBestSubject" expression="&quot;Science&quot;"/>
                                                    <assign variable="tiedBestScore" expression="mathScore"/>
                                                    <assign variable="tiedBestScore" expression="englishScore"/>
                                                    <assign variable="tiedBestScore" expression="scienceScore"/>
                                                </then>
                                                <else>
                                                    <assign variable="tiedBestSubject" expression="&quot;Math&quot;"/>
                                                    <assign variable="tiedBestSubject" expression="&quot;English&quot;"/>
                                                    <assign variable="worstSubject" expression="&quot;Science&quot;"/>
                                                    <assign variable="tiedBestScore" expression="mathScore"/>
                                                    <assign variable="tiedBestScore" expression="englishScore"/>
                                                    <assign variable="worstScore" expression="scienceScore"/>
                                                </else>
                                            </if>
                                        </then>
                                        <else>
                                            <if expression="englishScore == scienceScore">
                                                <then>
                                                    <assign variable="tiedBestSubject" expression="&quot;Science&quot;"/>
                                                    <assign variable="tiedBestSubject" expression="&quot;English&quot;"/>
                                                    <assign variable="worstSubject" expression="&quot;Math&quot;"/>
                                                    <assign variable="tiedBestScore" expression="scienceScore"/>
                                                    <assign variable="tiedBestScore" expression="englishScore"/>
                                                    <assign variable="worstScore" expression="mathScore"/>
                                                </then>
                                                <else>
                                                    <assign variable="tiedBestSubject" expression="&quot;Science&quot;"/>
                                                    <assign variable="tiedBestSubject" expression="&quot;Math&quot;"/>
                                                    <assign variable="worstSubject" expression="&quot;English&quot;"/>
                                                    <assign variable="tiedBestScore" expression="scienceScore"/>
                                                    <assign variable="tiedBestScore" expression="mathScore"/>
                                                    <assign variable="worstScore" expression="englishScore"/>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <output expression="&quot;Looks like you've already finished the quiz! Would you like to see the leaderboard? (Yes or No):&quot;" newline="True"/>
            <input variable="leaderboardOption"/>
            <call expression="leaderboard(bestSubject, secondBestSubject, worstSubject, worstScore, secondBestScore, bestScore, leaderboardOption, tiedBestSubject, tiedBestScore, tiedWorstScore, tiedWorstSubject)"/>
        </body>
    </function>
    <function name="englishQuiz" type="Integer" variable="englishScore">
